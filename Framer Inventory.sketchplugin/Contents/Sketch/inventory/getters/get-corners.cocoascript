
var getCornerRadius = function(layer) {

	var localType = exportableType(layer)

	if (localType == 1) {
		return nil
	}
	else if (localType == 2){
		var shape = layer.layers().firstObject()
		if(shape && shape.isKindOfClass(MSRectangleShape)) {
			return shape.cornerRadiusFloat()
		}
		return nil
	}
	else if (localType == 3){
		return "100%"
	}

	return nil
}
